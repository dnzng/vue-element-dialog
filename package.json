{
  "name": "vue-element-dialog",
  "version": "0.1.1",
  "description": "An imperative dialog for el-dialog in element-ui@2",
  "license": "MIT",
  "main": "dist/VueElementDialog.umd.min.js",
  "module": "dist/VueElementDialog.umd.js",
  "files": [
    "src",
    "dist/*.js"
  ],
  "keywords": [
    "vue",
    "element-ui",
    "el-dialog",
    "imperative"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./examples/main.js --open",
    "build": "vue-cli-service build --name VueElementDialog --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint src examples",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edwdzhang/vue-element-dialog.git"
  },
  "homepage": "https://github.com/edwdzhang/vue-element-dialog#readme",
  "bugs": {
    "url": "https://github.com/edwdzhang/vue-element-dialog/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.15.2",
    "element-ui": "^2.15.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.11"
  }
}
