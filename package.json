{
  "name": "vue-element-dialog",
  "version": "1.0.3",
  "description": "An imperative dialog for el-dialog component in element-ui based on Vue@2.x",
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Dylan Zhang",
  "license": "MIT",
  "homepage": "https://github.com/dnzng/vue-element-dialog#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnzng/vue-element-dialog.git"
  },
  "bugs": {
    "url": "https://github.com/dnzng/vue-element-dialog/issues"
  },
  "keywords": [
    "vue",
    "element-ui",
    "el-dialog",
    "imperative"
  ],
  "scripts": {
    "dev": "vite dev --open",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "element-ui": ">=2.12.0",
    "vue": ">=2.6 <3"
  },
  "devDependencies": {
    "@dnzng/eslint-config": "^0.1.4",
    "@vitejs/plugin-vue2": "^2.2.0",
    "bumpp": "^9.1.1",
    "element-ui": "^2.15.13",
    "eslint": "^8.47.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "vue": "^2.7.14"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm typecheck"
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json}": [
      "eslint --fix"
    ]
  }
}
